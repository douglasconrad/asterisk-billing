{
  "author": "Douglas Conrad <douglas.conrad@opens.com.br>",
  "name": "asterisk-billing",
  "description": "A way to genarate an external billing system for asterisk",
  "license": "Apache-2.0",
  "version": "1.0.0",
  "homepage": "http://github.com/douglasconrad/asterisk-billing",
  "repository": {
    "type": "git",
    "url": "git://github.com/douglasconrad/asterisk-billing.git"
  },
	"scripts": {  
		"start" : "node_modules/forever/bin/forever start -l billing.log -a index.js" ,
		"stop" : "node_modules/forever/bin/forever stop index.js",
		"status" : "node_modules/forever/bin/forever list" 
	},
  "main": "index.js",
  "keywords": [
    "billing",
    "queue",
    "asterisk",
    "worker"
  ],
  "engines": {
    "node": ">=0.10.0"
  },
  "dependencies": {
    "forever": "latest",
    "request": "latest",
    "asterisk-manager": "latest",
    "util": "latest",
    "bunyan": "latest",
    "rx": "latest",
    "superagent": "latest",
    "connect": "latest",
    "body-parser": "latest",
    "q": "latest",
    "http": "latest",
    "ini": "latest",
    "fs": "latest"
  }
}
